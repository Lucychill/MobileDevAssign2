<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#8BC34A">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="589dp"
        android:layout_height="124dp"
        android:background="#409444"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="16dp">

        <TextView
            android:id="@+id/textView"
            android:layout_width="102dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="@string/menu_title"
            android:textAlignment="center"
            android:textSize="32sp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="98dp"
            android:layout_height="107dp"
            android:layout_marginTop="16dp"
            android:layout_weight="1"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/healthyeatingapplogo" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="233dp"
        android:layout_height="61dp"
        android:text="@string/items_title"
        android:textAlignment="center"
        android:textSize="34sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="140dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/starter_title"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="213dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mains_title"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="248dp"
        tools:layout_editor_absoluteY="213dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="149dp"
        android:layout_height="138dp"
        android:background="#4CAF50"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/chicken_salad"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="459dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="151dp"
        android:layout_height="175dp"
        android:background="#4CAF50"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/veggie_wellington"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="663dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="153dp"
        android:layout_height="141dp"
        android:background="#4CAF50"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/chicken_fried_rice"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="256dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dessert_title"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="461dp"
        tools:layout_editor_absoluteY="213dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="165dp"
        android:layout_height="139dp"
        android:background="#4CAF50"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/vegetable_soup"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="256dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="136dp"
        android:layout_height="124dp"
        android:background="#4CAF50"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/grilled_carrots"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="473dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="128dp"
        android:layout_height="128dp"
        android:background="#4CAF50"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/avocado_side_salad"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="675dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="119dp"
        android:layout_height="125dp"
        android:background="#4CAF50"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/yogurt_dessert"
        tools:layout_editor_absoluteX="438dp"
        tools:layout_editor_absoluteY="255dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="136dp"
        android:layout_height="119dp"
        android:background="#4CAF50"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/angel_food_cake_with_oranges"
        tools:layout_editor_absoluteX="438dp"
        tools:layout_editor_absoluteY="478dp" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="128dp"
        android:layout_height="124dp"
        android:background="#4CAF50"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/vegetable_pudding"
        tools:layout_editor_absoluteX="438dp"
        tools:layout_editor_absoluteY="675dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>